// Q7: Write a program to swap two numbers without using a third variable.

#include<stdio.h>
int main()
{
    
    int x,y,z;
    printf("Enter first number\n");
    scanf("%d",&x);
    printf("Enter second number\n");
    scanf("%d",&y);
    printf("Your  numbers before swapping are x=%d,y=%d\n",x,y);
    printf("Your  numbers after swaping digits are x=%d,y=%d\n",y,x);
    return 0;
}
